{
  "private": true,
  "name": "rx-workerpool",
  "version": "1.0.0-a.1",
  "author": "retueze",
  "license": "MIT",
  "description": "Worker pool implementation for RxJS.",
  "keywords": [
    "worker",
    "pool",
    "workerpool",
    "parallel",
    "rxjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/retueZe/rx-workerpool"
  },
  "files": [
    "build"
  ],
  "type": "module",
  "types": "build/index.d.ts",
  "module": "build/index.js",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint .",
    "build:dev": "tsc --inlineSourceMap --removeComments",
    "build:prod": "npm run lint && rollup -c",
    "clean": "rm -rf build",
    "prepare": "npm run clean && npm run build:prod",
    "postpublish": "npm run clean"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "rollup": "^3.26.3",
    "rollup-plugin-dts": "^5.3.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "browser-or-node": "^2.1.1",
    "rxjs": "^7.0.0"
  }
}
